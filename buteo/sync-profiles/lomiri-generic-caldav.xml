<?xml version="1.0" encoding="UTF-8"?>
<profile name="lomiri-generic-caldav" type="sync" >
    <key name="category" value="calendar"/>
    <key name="remote_service_name" value="lomiri-generic-caldav"/>
    <key name="destinationtype" value="online"/>
    <key name="enabled" value="true"/>
    <key name="hidden" value="false"/>
    <key name="use_accounts" value="true"/>

    <profile type="client" name="caldav">
        <key value="two-way" name="Sync Direction"/>
        <key value="caldav" name="Sync Protocol"/>
        <key value="HTTP" name="Sync Transport"/>
        <key value="prefer remote" name="conflictpolicy" />
    </profile>

    <schedule enabled="false" interval="" days="1,2,3,4,5,6,7" syncconfiguredtime="" time="05:00:00" />
</profile>
